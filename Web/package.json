{
  "name": "scotlandsmountains",
  "version": "1.0.0",
  "description": "Scotland's Mountains",
  "keywords": [
    "Scotland",
    "mountains"
  ],
  "main": "app.js",
  "scripts": {
    "test": "jest --watch",
    "debug": "node ./node_modules/webpack/bin/webpack --config=./webpack.dev.config.js --colors --progress --watch",
    "build": "node ./node_modules/webpack/bin/webpack --config=./webpack.prod.config.js --colors --progress",
    "start": "node server.js",
    "encrypt": "encrypt ./src/config.js ./src/config.js.cast5",
    "decrypt": "decrypt ./src/config.js.cast5 ./src/config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/graham-miller/scotlandsmountains"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/material-ui"
    ]
  },
  "author": "Graham Miller",
  "license": "MIT",
  "dependencies": {
    "babel-jest": "^9.0.3",
    "babel-preset-jest": "^1.0.0",
    "jest-cli": "^0.10.2",
    "jquery": "^2.2.1",
    "leaflet": "^0.7.7",
    "material-ui": "^0.15.0-alpha.2",
    "normalize.css": "^4.0.0",
    "react": "^15.0.1",
    "react-addons-test-utils": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-router": "^2.0.1",
    "react-tap-event-plugin": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.0",
    "babel-jest": "^10.0.2",
    "babel-loader": "^6.2.2",
    "babel-polyfill": "^6.6.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-jest": "^10.0.2",
    "babel-preset-react": "^6.5.0",
    "config-leaf": "^0.2.0",
    "copy-webpack-plugin": "^2.1.1",
    "css-loader": "^0.23.1",
    "es6-promise": "^3.1.2",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.8.1",
    "jest-cli": "^0.10.2",
    "json-server": "^0.8.8",
    "less": "^2.6.1",
    "less-loader": "^2.2.3",
    "open": "^0.0.5",
    "react-addons-test-utils": "^15.0.1",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1",
    "webpack-notifier": "^1.3.0"
  }
}
