{
  "name": "scotlandsmountains",
  "version": "1.0.0",
  "description": "Explore Scotland's Mountains",
  "keywords": [ "Scotland", "mountains", "maps" ],
  "author": "Graham Miller",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graham-miller/scotlandsmountains.git"
  },
  "license": "MIT",
  "scripts": {
    "start": "node Client/scripts/start.js",
    "build": "node Client/scripts/build.js",
    "test": "node Client/scripts/test.js --env=jsdom",
    "test:lint":        "node node_modules/eslint/bin/eslint . --ext .js",
    "test:lint:output": "node node_modules/eslint/bin/eslint . --ext .js --output-file eslint_result.txt",
    "test:lint:fix":    "node node_modules/eslint/bin/eslint . --ext .js --fix"
  },
  "devDependencies": {
    "autoprefixer": "6.5.3",
    "babel-core": "6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^17.0.2",
    "babel-loader": "6.2.8",
    "babel-preset-react-app": "^1.0.0",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "css-loader": "0.26.0",
    "detect-port": "1.0.5",
    "dotenv": "2.0.0",
    "eslint": "^3.10.2",
    "eslint-config-react-app": "^0.3.0",
    "eslint-loader": "1.6.1",
    "eslint-plugin-flowtype": "^2.25.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.1",
    "eslint-plugin-react": "^6.7.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "filesize": "3.3.0",
    "find-cache-dir": "0.1.1",
    "fs-extra": "1.0.0",
    "gzip-size": "3.0.0",
    "html-webpack-plugin": "2.24.1",
    "http-proxy-middleware": "0.17.2",
    "jest": "^17.0.3",
    "json-loader": "0.5.4",
    "node-sass": "^3.13.0",
    "object-assign": "4.1.0",
    "path-exists": "3.0.0",
    "postcss-loader": "1.1.1",
    "promise": "7.1.1",
    "react-dev-utils": "^0.3.0",
    "recursive-readdir": "2.1.0",
    "rimraf": "2.5.4",
    "sass-loader": "^4.0.2",
    "strip-ansi": "3.0.1",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.3",
    "webpack-notifier": "^1.4.1",
    "webpack-dev-server": "1.16.2",
    "whatwg-fetch": "2.0.1"
  },
  "dependencies": {
    "bootstrap": "4.0.0-alpha.4",
    "leaflet": "^1.0.2",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-icons": "^2.2.1",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "tether": "^1.3.7"
  },
  "jest": {
    "moduleFileExtensions": [ "jsx", "js", "json" ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>\\Client\\config\\jest\\FileStub.js",
      "^.+\\.css$": "<rootDir>\\Client\\config\\jest\\CSSStub.js",
      "^.+\\.scss$": "<rootDir>\\Client\\config\\jest\\SCSSStub.js"
    },
    "setupFiles": [
      "<rootDir>\\Client\\config\\polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules)/"
    ],
    "testEnvironment": "node",
    "preprocessorIgnorePatterns": [ "/(?!node_modules/react-icons)node_modules/" ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": "react-app",
    "parserOptions": {
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true,
        "jsx": true
      },
      "sourceType": "module"
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "indent": [
        "error",
        4
      ],
      "linebreak-style": [
        "error",
        "windows"
      ],
      "quotes": [
        "error",
        "double"
      ],
      "semi": [
        "error",
        "always"
      ]
    }
  },
  "homepage": "https://scotlandsmountains.github.io",
  "proxy": "http://localhost:54944/",
  "-vs-binding": { "ProjectOpened": [ "start", "test" ] }
}